@model IEnumerable<Assignment2.Models.qualifyGrid>
@{
    WebGrid grid = new WebGrid(Model);
    ViewBag.Title = "Home Page";
}

@{
    ViewBag.Title = "QualificationCrew";
}

<h2>QualificationCrew</h2>

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>

            <p>

                <h2>People</h2> 
                @grid.GetHtml(columns: grid.Columns(
                    grid.Column("name", "Crew Name"),
                    grid.Column("aircrafts", "Aircraft Model")
                
                 ))
                
            </p>
        </div>
    </section>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
